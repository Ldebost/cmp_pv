{
  "name": "cmp_pv",
  "version": "1.0.0",
  "description": "ParuVendu.fr Consent Management Platform",
  "homepage": "https://github.com/Ldebost/cmp_pv",
  "main": "cmp.js",
  "scripts": {
    "dev": "ws",
    "build-cmp": "uglifyjs polyfills/base64.polyfill.js polyfills/assign.polyfill.js cmp.js --compress --mangle --source-map --output cmp.min.js",
    "build-stub": "uglifyjs stub.js --compress --mangle -o stub.min.js",
    "build-portal": "uglifyjs polyfills/assign.polyfill.js portal.js --compress --mangle -o portal.min.js",
    "deploy": "npm run build-cmp && npm run build-stub && npm run build-portal",
    "test": "uglifyjs cmp.js --compress sequences,dead_code,conditionals,booleans,unused,if_return,join_vars --mangle toplevel --mangle-props reserved=[__cmp,init,getVendorConsents,getConsentData,ping,getPublisherConsents,getVendorList,showConsentUi,data,dataPub,version,created,lastUpdated,cmpId,cmpVersion,consentScreen,consentLanguage,vendorListVersion,purposesAllowed,maxVendorId,encodingType,bitField,defaultConsent,numEntries,vendor_1,metadata_1,publisher_1] -o cmp.min.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "local-web-server": "^2.5.2",
    "uglify-js": "^3.4.1"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/Ldebost/cmp_pv"
  }
}
